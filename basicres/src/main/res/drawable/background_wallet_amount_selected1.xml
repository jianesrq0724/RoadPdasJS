<?xml version="1.0" encoding="utf-8"?>
<!-- 钱包 企业账户选中时-->
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

    <solid android:color="@color/btn_state_enabled_true"  />
    <corners android:radius="5dp" />
    <stroke
        android:width="1dp"
        android:color="@color/btn_state_enabled_true" />
    <gradient
        android:startColor="@color/btn_state_enabled_true"
        android:centerColor="@color/background_deep_blue"
        android:endColor="@color/background_deep_blue"
        android:useLevel="true"
        android:angle="270"
        android:type="linear"
        android:centerX="0"
        android:centerY="0"
        android:gradientRadius="270"/>
</shape>